{
    "builds": [{
        "src": "blackbox/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9", "installCommand": "pip install -r requirements.txt" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "blackbox/wsgi.py"
        }
    ]
}